<table>
  <% @schools.each do |school| %>
    <tr>
      <td><%= link_to school.name, staff_school_path(school) %></td>
      <td><%= school.address %></td>
      <td><%= link_to "Edit", edit_staff_school_path(school) %></td>
      <td><%= link_to "Delete", staff_school_path(school), method: :delete %></td>
    </tr>
  <% end %>
</table>